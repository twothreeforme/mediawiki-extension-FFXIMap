{
	"name": "FFXIMap",
	"version": "1.0",
	"author": [
		"Shocks"
	],
	"url": "https://github.com/twothreeforme/mediawiki-extension-FFXIMap",
	"descriptionmsg": "Adds <ffximap> tag for MediaWiki use. Forked from https://github.com/atitd-community/mediawiki-emap. ",
	"license-name": "MIT",
	"type": "FFXI",
	"requires": {
		"MediaWiki": ">= 1.25.0"
	},
	"AutoloadClasses": {
		"FFXIMap": "FFXIMap_body.php"
	},
	"ExtensionMessagesFiles": {
		"FFXIMap": "FFXIMap.magic.php"
	},
	"ResourceModules": {
		"ext.FFXIMap.styles": {
			"styles": "ext.FFXIMap.leaflet.css",
			"targets": [
				"mobile",
				"desktop"
			]
		},
		"ext.FFXIMap": {
			"scripts": [
				"ext.FFXIMap.leaflet.js",
				"ext.FFXIMap.babel.js"
			],
			"targets": [
				"mobile",
				"desktop"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "FFXIMap/modules"
	},
	"Hooks": {
		"ParserFirstCallInit": "FFXIMap::onParserSetup",
		"BeforePageDisplay": "FFXIMap::onBeforePageDisplay",
		"ParserAfterTidy": "FFXIMap::onParserAfterTidy"
	},
	"manifest_version": 1
}
